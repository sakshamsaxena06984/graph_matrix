//Binary operator overloading

#include<iostream>
using namespace std;
class complex
{
    int real;
    int img;
  public:
   void setdata(int x,int y)
   {
       real = x;
       img = y;
   }
   void getdata()
   {
       cout << real << " " << img << "i" << endl;
   }
   complex operator +(complex c1)
   {
       complex temp;
       temp . real = real + c1 . real;
       temp . img = img + c1 . img;
       return temp;
   }
};
int main()
{
    complex s1 , s2 , s3;
    s1 . setdata(12,34);
    s1 . getdata();
    cout << endl;
    s2 . setdata(1,3);
    s2 . getdata();
    cout << endl;
    s3 = s1 + s2;//s3=s1.operator +(s2); //no need to change in the function defination
    s3 . getdata();
}



